        },
        "name": "winnie"
      },
      "inputsExtra": {
        "helloWorld": "com.ztianzeng.agouti.core.HelloWorld",
        "name": "java.lang.String"
      }
    },
    {
      "target": "url:http://helloWorld.test.hello",
      "alias": "d2",
      "method": "getB",
      "inputsExtra": {
        "helloWorld": "com.ztianzeng.agouti.core.HelloWorld",
        "name": "java.lang.String"
      },
      "timeout": 3000,
      "inputs": {
        "helloWorld": {
          "name": "csophys",
          "sex": "boy"
        },
        "name": "winnie"
      }
    }
  ]
